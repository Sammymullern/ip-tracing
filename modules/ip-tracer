if [ -d /data/data/com.termux/files/usr/share ];then
  cd /data/data/com.termux/files/usr/share/location-tracer
  exec bash trace $@
elif [ -d /usr/share ];then
  cd /usr/share/location-tracer
  exec bash trace $@
fi
